{
	"name": "knowyourcode",
	"displayName": "KnowYourCode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.dadJoke",
		"onCommand:extension.knowyourcode",
		"onCommand:catCoding.start",
		"onCommand:catCoding.doRefactor",
		"onWebviewPanel:catCoding",
		"onCommand:extension.statusBar"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.dadJoke",
				"title": "Dad Joke"
			},
			{
				"command": "extension.knowyourcode",
				"title": "Know Your Code"
			},
			{
				"command": "extension.createGist",
				"title": "Create Gist"
			},
			{
				"command": "catCoding.start",
				"title": "Start cat coding session",
				"category": "Cat Coding"
			},
			{
				"command": "catCoding.doRefactor",
				"title": "Do some refactoring",
				"category": "Cat Coding",
				"command": "extension.statusBar",
				"title": "Status Bar"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.createGist",
					"group": "githubgist"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@octokit/rest": "^16.35.0",
		"superagent": "^5.1.2"
	}
}
